{"name":"Armes","pages":[{"sort":100000,"name":"Armes de mêlée","type":"text","title":{"show":true,"level":1},"image":{},"text":{"format":1,"content":"<table class=\"table gear-table\"><thead><tr class=\"table-header-row\"><th class=\"table-column-header\" scope=\"col\">Type</th><th class=\"table-column-header\" scope=\"col\">Damage</th><th class=\"table-column-header\" scope=\"col\">Min For.</th><th class=\"table-column-header\" scope=\"col\">Weight</th><th class=\"table-column-header\" scope=\"col\">Note</th><th class=\"table-column-header\" scope=\"col\">Cost</th></tr></thead><tbody><tr><td>Arme de mêlée légère</td><td>@str+1d4</td><td>d4</td><td>1</td><td>Dissimulable</td><td>50</td></tr><tr><td>Arme de mêlée intermédiaire</td><td>@str+1d6</td><td>d6</td><td>2</td><td></td><td>50</td></tr><tr><td>Arme de mêlée lourde</td><td>@str+1d8</td><td>d8</td><td>3</td><td></td><td>100</td></tr><tr><td>Arme de mêlée très lourde</td><td>@str+1d10</td><td>d10</td><td>4</td><td></td><td>500</td></tr></tbody></table>","markdown":""},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"ownership":{"default":-1},"flags":{},"_id":"1TwT7aEOKBsgmqkg"},{"sort":100000,"name":"Arme à distance","type":"text","title":{"show":true,"level":1},"image":{},"text":{"format":1,"content":"<p></p><table class=\"table gear-table\"><thead><tr class=\"table-header-row\"><th class=\"table-column-header\" scope=\"col\">Type</th><th class=\"table-column-header\" scope=\"col\">Portée</th><th class=\"table-column-header\" scope=\"col\">Dégâts</th><th class=\"table-column-header\" scope=\"col\">PA</th><th class=\"table-column-header\" scope=\"col\">CdT</th><th class=\"table-column-header\" scope=\"col\">Tirs</th><th class=\"table-column-header\" scope=\"col\">Min For.</th><th class=\"table-column-header\" scope=\"col\">Weight</th><th class=\"table-column-header\" scope=\"col\">Note</th><th class=\"table-column-header\" scope=\"col\">Cost</th></tr></thead><tbody><tr><th class=\"table-column-head